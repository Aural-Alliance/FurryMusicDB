{
  "name": "traaaashpanda/furrymusicdb",
  "require": {
    "php": "^8.2",
    "ext-PDO": "*",
    "ext-fileinfo": "*",
    "ext-json": "*",
    "akrabat/ip-address-middleware": "^2.1",
    "composer/ca-bundle": "^1.2",
    "doctrine/dbal": "^4",
    "doctrine/migrations": "^3",
    "doctrine/orm": "^3",
    "guzzlehttp/guzzle": "^7.0",
    "intervention/image": "^3",
    "league/flysystem": "^3.16",
    "league/oauth2-client": "^2.7",
    "mezzio/mezzio-session": "^1.13",
    "mezzio/mezzio-session-cache": "^1.11",
    "monolog/monolog": "^3",
    "nesbot/carbon": "^3",
    "pagerfanta/doctrine-collections-adapter": "^4.2",
    "pagerfanta/doctrine-orm-adapter": "^4.2",
    "php-di/php-di": "^7.0",
    "psr/simple-cache": ">1",
    "ramsey/uuid": "^4.7",
    "slim/http": "^1",
    "slim/slim": "^4",
    "symfony/cache": "^7",
    "symfony/console": "^7",
    "symfony/property-access": "^7",
    "symfony/serializer": "^7",
    "symfony/validator": "^7"
  },
  "require-dev": {
    "php-parallel-lint/php-console-highlighter": "^1",
    "php-parallel-lint/php-parallel-lint": "^1.3",
    "phpstan/phpstan": "^1.5",
    "phpstan/phpstan-doctrine": "^1.3",
    "roave/security-advisories": "dev-latest",
    "slevomat/coding-standard": "^8",
    "squizlabs/php_codesniffer": "^3.5",
    "symfony/var-dumper": "^7"
  },
  "scripts": {
    "cleanup-and-test": [
      "@phpcbf",
      "@dev-test"
    ],
    "dev-test": [
      "@phplint",
      "@phpstan",
      "@phpcs"
    ],
    "phplint": "parallel-lint . --exclude vendor",
    "phpstan": "phpstan analyze --memory-limit=1G",
    "phpcs": "phpcs",
    "phpcbf": "phpcbf"
  },
  "authors": [
    {
      "name": "Buster Neece",
      "email": "buster@busterneece.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "src"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "php-http/discovery": true
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
