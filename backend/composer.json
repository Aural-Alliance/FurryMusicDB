{
  "name": "traaaashpanda/furrymusicdb",
  "require": {
    "php": "^8.2",
    "ext-PDO": "*",
    "ext-fileinfo": "*",
    "ext-json": "*",
    "akrabat/ip-address-middleware": "^2.1",
    "auth0/auth0-php": "^8.6",
    "composer/ca-bundle": "^1.2",
    "doctrine/annotations": "^2.0",
    "doctrine/dbal": "^3",
    "doctrine/migrations": "^3",
    "doctrine/orm": "^2",
    "guzzlehttp/guzzle": "^7.0",
    "monolog/monolog": "^3",
    "nesbot/carbon": "^2.45",
    "pagerfanta/doctrine-collections-adapter": "^4.2",
    "pagerfanta/doctrine-orm-adapter": "^4.2",
    "php-di/php-di": "^7.0",
    "psr/simple-cache": ">1",
    "slim/http": "^1",
    "slim/slim": "^4",
    "symfony/cache": "^6",
    "symfony/console": "^6",
    "symfony/property-access": "^6.3",
    "symfony/serializer": "^6.3",
    "symfony/validator": "^6.3"
  },
  "require-dev": {
    "php-parallel-lint/php-console-highlighter": "^1",
    "php-parallel-lint/php-parallel-lint": "^1.3",
    "phpstan/phpstan": "^1.5",
    "phpstan/phpstan-doctrine": "^1.3",
    "roave/security-advisories": "dev-latest",
    "slevomat/coding-standard": "^8",
    "squizlabs/php_codesniffer": "^3.5",
    "symfony/var-dumper": "^6.3"
  },
  "scripts": {
    "cleanup-and-test": [
      "@phpcbf",
      "@dev-test"
    ],
    "dev-test": [
      "@phplint",
      "@phpstan",
      "@phpcs"
    ],
    "phplint": "parallel-lint . --exclude vendor",
    "phpstan": "phpstan analyze --memory-limit=1G",
    "phpcs": "phpcs",
    "phpcbf": "phpcbf"
  },
  "authors": [
    {
      "name": "Buster Neece",
      "email": "buster@busterneece.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "src"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "php-http/discovery": true
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
